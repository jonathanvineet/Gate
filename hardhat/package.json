  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "dotenv": "^16.4.5",
    "hardhat": "^2.22.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2"
  }
}
